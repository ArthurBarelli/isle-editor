{
  "presets": [
     "es2015",
     "react",
     "stage-0"
  ],
  "plugins": [
    "add-module-exports",
    "transform-react-jsx"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "plugins": [
        [
          "resolver", {
            "resolveDirs": [
                "app",
                "node_modules/@stdlib/stdlib/lib/node_modules"
              ]
            }
        ]
      ]
    }
  }
}
